<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="BitBag\SyliusMultiVendorMarketplacePlugin\Entity\VendorImage" table="bitbag_vendor_image">
        <many-to-one field="owner" target-entity="BitBag\SyliusMultiVendorMarketplacePlugin\Entity\Vendor" inversed-by="images">
                <join-column name="owner_id" referenced-column-name="id" nullable="false" on-delete="CASCADE" />
        </many-to-one>
    </entity>
</doctrine-mapping>
