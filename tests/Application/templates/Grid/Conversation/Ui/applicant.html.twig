{% set userRole = '' %}
{% for role in data.roles %}
    {% set userRole = role %}
{% endfor %}

{% if userRole is same as 'ROLE_VENDOR' %}
    <div>
        {{ data.firstName }} {{ data.lastName }}
    </div>
{% endif %}

<div>
    {{ 'mvm.ui.conversations_listing.username'|trans }}: {{ data.username }}
</div>

